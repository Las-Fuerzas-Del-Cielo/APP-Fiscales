{
  "swagger": "2.0",
  "info": {
    "title": "LLA Fraud detection API",
    "description": "API for the LLA Fraud detection project",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/reports": {
      "get": {
        "description": "",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/reports/{id}": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/audit": {
      "post": {
        "description": "",
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    },
    "/voting-tables/{id}": {
      "get": {
        "description": "Obtiene información sobre la mesa en particular",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "default": "1244",
            "description": "Id de la Mesa"
          },
          {
            "name": "anioEleccion",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 2019,
            "description": "Año del Ciclo Electoral"
          },
          {
            "name": "tipoRecuento",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 1,
            "description": "Tipo de recuento que se quiere consultar. Utilizar el valor '1' para Recuento Provisional."
          },
          {
            "name": "tipoEleccion",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 1,
            "description": "Tipo de elección a consultar. Utilizar el valor '1' para PASO, '2' para Elecciones Generales y '3' para Segunda Vuelta."
          },
          {
            "name": "categoriaId",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 2,
            "description": "Id de la categoría electoral para la cual se están solicitando datos."
          },
          {
            "name": "distritoId",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 6,
            "description": "Id del distrito"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/voting-tables/{id}/report-missing-auditor": {
      "post": {
        "description": "Report a missing fiscal at a specific voting table",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "The ID of the voting table"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "fiscalId": {
                  "type": "string",
                  "description": "The ID of the fiscal"
                },
                "escuelaId": {
                  "type": "string",
                  "description": "The ID of the school"
                }
              },
              "required": ["fiscalId", "escuelaId"]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Report successfully received",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                },
                "resultadoS3": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request data"
          },
          "401": {
            "description": "Unauthorized access"
          },
          "404": {
            "description": "Voting table or school not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/voting-tables": {
      "get": {
        "description": "",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user": {
      "post": {
        "description": "",
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      },
      "get": {
        "description": "",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user/{id}": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user/{id}/roles": {
      "get": {
        "description": "",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/upload": {
      "post": {
        "summary": "Upload a file to the server",
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "in": "formData",
            "name": "file",
            "type": "file",
            "required": true,
            "description": "File to upload"
          }
        ],
        "responses": {
          "200": {
            "description": "File uploaded successfully"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  }
}